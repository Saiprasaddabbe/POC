package com.neosoft;

interface Test{
	public void fun1();
	
}

public class Main {
	
	public static void fun2() {
		System.out.println("inside fun2");
	}
	
	
	public static void main(String[] args) {
		
		Test t1 = ()->System.out.println("inside fun1");
		t1.fun1();
		
		Test t2 = Main::fun2;
		t2.fun1();
		
		
		
		
	}

}
