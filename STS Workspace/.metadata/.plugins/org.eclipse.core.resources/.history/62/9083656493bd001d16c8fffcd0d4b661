package com.neosoft;

class A{
	public void funA() {
		System.out.println("funA");
	}
	public void funX() {
		System.out.println("X");
	}
}
class B extends A{
	public void funA() {
		System.out.println("funB");
	}
}
class C extends B{
	
	public void funA() {
		System.out.println("funC");
	}
}

public class Main {

	public static void main(String[] args) {
  
		A a1 = new A();
		B b1 = new B();
		C c1 = new C();
		a1.funA();
		b1.funA();
		c1.funA();
		b1.funX();
		c1.funX();
		
		
	}

}
