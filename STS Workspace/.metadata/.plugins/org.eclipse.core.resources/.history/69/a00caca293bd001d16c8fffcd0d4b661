package com.neosoft;

import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		
		List<Integer> num = new ArrayList<>();
		
		num.add(1);
		num.add(2);
		num.add(23);
		num.add(10);
		
		for(int n: num) {
			
			if(n == 2) {
				num.add(60);
			}
		}
		System.out.println(num);
		
	}

}
